{
  "name": "GitHub Feed Filter",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Enables filtering out GitHub feeds by projects, commits, comments and issues",
  "icons": { "16": "ghf16.png", "48": "ghf48.png", "128": "ghf128.png" },
  "content_scripts": [
    {
      "matches": ["https://github.com/", "https://github.com/dashboard", "https://github.com/#" ],
      "js": ["jquery-1.4.2.min.js", "filter.js"]
    }
  ]
}
